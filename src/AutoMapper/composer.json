{
    "name": "jane-php/automapper",
    "type": "library",
    "description": "Jane AutoMapper Component",
    "keywords": [],
    "homepage": "https://github.com/janephp/janephp",
    "license": "MIT",
    "authors": [
        {
            "name": "Joel Wurtz",
            "email": "jwurtz@jolicode.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "doctrine/inflector": "^1.4",
        "nikic/php-parser": "^4.0",
        "symfony/property-info": "~5.1"
    },
    "require-dev": {
        "doctrine/annotations": "~1.0",
        "phpdocumentor/reflection-docblock": "^3.0 || ^4.0",
        "symfony/serializer": "^4.2 || ^5.0"
    },
    "suggest": {
        "symfony/serializer": "Allow to bridge mappers to normalizer and denormalizer"
    },
    "conflict": {
        "symfony/serializer": "<4.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Jane\\AutoMapper\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "Bundle/Tests/Resources/app/AppKernel.php"
        ]
    },
    "config": {
        "process-timeout": 1800,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
