{
    "name": "jane-php/automapper",
    "type": "library",
    "description": "Jane AutoMapper Component",
    "keywords": [],
    "homepage": "https://github.com/janephp/janephp",
    "license": "MIT",
    "authors": [
        {
            "name": "Joel Wurtz",
            "email": "jwurtz@jolicode.com"
        },
        {
            "name": "Baptiste Leduc",
            "email": "baptiste.leduc@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "doctrine/inflector": "^1.4 || ^2.0",
        "nikic/php-parser": "^4.0",
        "symfony/property-info": "^5.2",
        "symfony/serializer": "^4.2 || ^5.0"
    },
    "require-dev": {
        "doctrine/annotations": "~1.0",
        "moneyphp/money": "^3.0",
        "phpdocumentor/reflection-docblock": "^3.0 || ^4.0 || ^5.0",
        "phpunit/phpunit": "^8.0",
        "symfony/uid": "^5.2"
    },
    "suggest": {
        "doctrine/annotations": "Docblock Annotations Parser",
        "phpdocumentor/reflection-docblock": "Allow to extract informations from PHP Doc blocks"
    },
    "conflict": {
        "symfony/serializer": "<4.2",
        "symfony/property-info": "5.1.6 || 5.1.7"
    },
    "extra": {
        "branch-alias": {
            "dev-next": "7-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Jane\\Component\\AutoMapper\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "process-timeout": 1800,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
